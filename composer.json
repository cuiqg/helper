{
  "name": "cuiqg/helper",
  "description": "个人常用辅助函数",
  "version": "1.0.0-dev",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "cuiqg",
      "email": "i@cuiqg.me"
    }
  ],
  "support": {
    "source": "https://github.com/cuiqg/helper"
  },
  "require": {
    "php": ">=7.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-SimpleXML": "*"
  },
  "autoload": {
      "psr-4": {
        "Cuiqg\\Helper\\": "src/"
      },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cuiqg\\Helper\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "repositories": [
    {
     "type": "composer",
      "url": "https://php.cnpkg.org"
    }
  ]
}
